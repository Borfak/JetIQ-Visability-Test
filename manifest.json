{
    "manifest_version": 3,
    "name": "Disable Test Visibility JetIQ",
    "version": "1.0.1",
    "author": "Borfak",
    "description": "Prevents JetIQ from tracking test visibility changes.",
    "permissions": [],
    "host_permissions": ["*://*/*"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["disable.js"]
        }
    ],
    "icons": {
        "48": "icon.png"
    }
}
